  keywords = {Categorical models of meaning,Categorical Semantics,Category Theory,Generalized relations,Generalized spans,Relations},
  author = {Coecke, Bob and Genovese, Fabrizio and Lewis, Martha and Marsden, Dan and Toumi, Alex}
}

